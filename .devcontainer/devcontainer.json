{
  "name": "Python Jupyter Dev Container",
  "image": "ghcr.io/anushapb/evodoodle:latest",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": [
    "ms-python.python",
    "ms-toolsai.jupyter"
  ],
  "forwardPorts": [8888],
  "remoteUser": "vscode",
  "features": {
    "ghcr.io/devcontainers/features/desktop-lite:1": {}
  }
}
